{
    "name": "jmikola/auto-login-bundle",
    "type": "symfony-bundle",
    "description": "Authenticate users in your Symfony app via a single query parameter (e.g. email and newsletter links).",
    "keywords": ["authentication", "auto-login", "login", "security"],
    "homepage": "https://github.com/jmikola/JmikolaAutoLoginBundle",
    "license": "MIT",
    "authors": [
        { "name": "Jeremy Mikola", "email": "jmikola@gmail.com" }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/config": "^2.2 || ^3.0 || ^4.0",
        "symfony/dependency-injection": "^2.2 || ^3.0 || ^4.0",
        "symfony/http-kernel": "^2.2 || ^3.0 || ^4.0",
        "symfony/security-bundle": "^2.2 || ^3.0 || ^4.0",
        "jmikola/auto-login": "^1.2.3"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^0.7.6",
        "symfony/browser-kit": "^2.2 || ^3.0 || ^4.0",
        "symfony/class-loader": "^2.2 || ^3.0 || ^4.0",
        "symfony/framework-bundle": "^2.2 || ^3.0 || ^4.0",
        "symfony/yaml": "^2.2 || ^3.0 || ^4.0"
    },
    "autoload": {
        "psr-0": { "Jmikola\\AutoLoginBundle": "" }
    },
    "target-dir": "Jmikola/AutoLoginBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
